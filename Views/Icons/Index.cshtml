@model IEnumerable<RezzQueue.Models.Icon>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.IconName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IconGlyph)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.IconName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IconGlyph)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.IconId }) |
            @Html.ActionLink("Details", "Details", new { id=item.IconId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.IconId })
        </td>
    </tr>
}

</table>
